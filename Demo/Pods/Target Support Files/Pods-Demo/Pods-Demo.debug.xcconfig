FRAMEWORK_SEARCH_PATHS = $(inherited) $(PODS_ROOT)/FlashPollsSDK "${PODS_ROOT}/FlashPollsSDK"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/FlashPollsSDK"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited)  -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/FlashPollsSDK"
OTHER_LDFLAGS = $(inherited) -framework "FlashPolls" -framework "Foundation" -framework "UIKit"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Demo
PODS_ROOT = ${SRCROOT}/Pods
